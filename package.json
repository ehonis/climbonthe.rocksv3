{
  "name": "climbonthe.rocks",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "npx dotenv -e .env.local -- prisma generate",
    "migrate:dev": "npx dotenv -e .env.local -- prisma migrate dev",
